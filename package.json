{
  "name": "vehicle-expense-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:capacitor": "next build",
    "cap:dev": "./scripts/capacitor-dev.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__/(?!integration|e2e)",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:security": "jest --testPathPattern=__tests__/security",
    "test:performance": "jest --testPathPattern=__tests__/performance",
    "test:accessibility": "jest --testPathPattern=__tests__/accessibility",
    "test:all": "npm run type-check && npm run lint && npm run test:ci",
    "audit:security": "npm audit --audit-level moderate",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.2",
    "@ionic/react": "^8.5.8",
    "@ionic/react-router": "^8.7.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "esm": "^3.2.25",
    "lightningcss": "^1.30.1",
    "mongodb": "^6.17.0",
    "mongoose": "^8.18.3",
    "next": "^15.3.5",
    "next-auth": "^4.24.11",
    "papaparse": "^5.5.3",
    "postcss": "^8.5.6",
    "postcss-nesting": "^13.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.2",
    "tailwindcss": "^4.1.12",
    "typescript": "^5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "node-mocks-http": "^1.17.2",
    "supertest": "^7.1.4"
  }
}
